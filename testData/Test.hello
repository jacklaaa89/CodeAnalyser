    /* testerMethod commenting. */
    void testerMethod(tester1, tester2) {
        try {
            switch(tester1) {
                case "hello" {
                    print(tester1);
                }
                default {
                    print(tester2);
                }
            }
        } catch(exception) {
            print(exception);
        }
    }

    /* testerMethod2 commenting. */
    void testerMethod2(tester1, tester2) {
        if(tester1 < tester2) { //line comment.
            print(tester1);
        } else {
            Math.calculate(tester2, tester1);
        }
    }

}