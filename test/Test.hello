/* classCommenting */
class Test {

    /* testerMethod commenting. */
    void testerMethod(tester1, tester2) {
        switch(tester1) {
            case "hello" {
                print(tester1);
            }
            default {
                print(tester2);
            }
        }
    }

    /* testerMethod2 commenting. */
    void testerMethod2(tester1, tester2) {
        if(tester1 < tester2) {
            print(tester1);
        }
    }

}